// swapping two numbers in C# without using temporary variable

using System;
 
class A {
    public static void Main()
    {
        int a = 1;
        int b = 2;
 
        a = a + b;
        b = a - b;
        a = a - b;
        Console.WriteLine("After swapping: a = " + a + ", b = " + b);
    }
}
